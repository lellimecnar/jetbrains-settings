<application>
  <component name="DiffManager">
    <enableFiles value="true" />
    <enableMerge value="true" />
    <filesTool value="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" newValue="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" />
    <mergeTool value="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" newValue="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" />
    <mergeToolParameters value="--merge --base %3 %1 %2" />
  </component>
  <component name="DiffSettings">
    <option name="PLACES_MAP">
      <entry key="ChangesView">
        <PlaceSettings>
          <option name="DIFF_TOOLS_ORDER">
            <list>
              <option value="com.intellij.openapi.vcs.changes.patch.tool.ApplyPatchDiffTool" />
              <option value="com.intellij.openapi.vcs.changes.patch.tool.PatchDiffTool" />
              <option value="org.jetbrains.idea.svn.difftool.properties.SvnPropertiesDiffTool" />
              <option value="org.jetbrains.idea.svn.difftool.SvnPhantomChangeDiffRequestProvider.SvnPhantomDiffTool" />
              <option value="org.jetbrains.idea.svn.difftool.SvnTreeConflictDiffRequestProvider.SvnTreeConflictDiffTool" />
              <option value="com.intellij.database.diff.SchemaDiffTool" />
              <option value="com.intellij.database.diff.TableDiffTool" />
              <option value="com.intellij.diff.tools.fragmented.UnifiedDiffTool" />
              <option value="com.intellij.diff.tools.simple.SimpleDiffTool" />
              <option value="com.intellij.diff.tools.binary.BinaryDiffTool" />
              <option value="com.intellij.diff.tools.dir.DirDiffTool" />
            </list>
          </option>
        </PlaceSettings>
      </entry>
    </option>
  </component>
  <component name="ExternalDiffSettings">
    <option name="DIFF_ENABLED" value="true" />
    <option name="DIFF_DEFAULT" value="true" />
    <option name="DIFF_EXE_PATH" value="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" />
    <option name="DIFF_PARAMETERS" value="--diff %1 %2" />
    <option name="MERGE_ENABLED" value="true" />
    <option name="MERGE_EXE_PATH" value="/Applications/Kaleidoscope.app/Contents/Resources/bin/ksdiff" />
    <option name="MERGE_PARAMETERS" value="--merge --base %3 %1 %2" />
    <option name="MERGE_TRUST_EXIT_CODE" value="true" />
  </component>
</application>